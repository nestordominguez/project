<%if notice%>
  <p id="notice" class="alert alert-success"><%= notice %></p>
<%end%>

<p class="col-md-4">
  <strong>User Email:</strong>
  <%= @user.email %>
</p>

<p class="col-md-4">
  <strong>User active:</strong>
  <%= @user.active %>
</p>

<div class="col-sm-12">
	<%= link_to 'Edit', edit_admin_user_path(@user), class: "col-md-1 links" %>
	<%= link_to 'Back', admin_users_path, class: "col-md-1 links" %>
</div>
